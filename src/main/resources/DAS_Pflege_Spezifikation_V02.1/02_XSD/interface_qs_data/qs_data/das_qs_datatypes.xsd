			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>in der Pflegeeinrichtung</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation>im Krankenhaus</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation>zuhause (vor dem Einzug)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="4">
				<xs:annotation>
					<xs:documentation>woanders</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
