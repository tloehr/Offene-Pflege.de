			<xs:documentation>Gesprächsteilnehmer</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:int">
			<xs:enumeration value="0">
				<xs:annotation>
					<xs:documentation>Keine der angegebenen</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>Bewohner/Bewohnerin</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation>Angehörige</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation>Betreuer/Betreuerin</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="4">
				<xs:annotation>
					<xs:documentation>andere Vertrauenspersonen, die nicht in der Einrichtung
						beschäftigt sind</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="enum_ausschlussgrund_type">
		<xs:annotation>
			<xs:documentation>Ausschlussgrund im Fall eines Minimaldatensatzes
				(MDS)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:int">
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>Einzugsdatum liegt weniger als 14 Tage vor dem
						Stichtag</xs:documentation>
				</xs:annotation>
