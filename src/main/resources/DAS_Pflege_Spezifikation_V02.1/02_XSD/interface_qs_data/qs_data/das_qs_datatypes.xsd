	<xs:simpleType name="enum_kuenstlicheErnaehrung_type">
		<xs:annotation>
			<xs:documentation>Künstliche Ernährung</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:int">
			<xs:enumeration value="0">
				<xs:annotation>
					<xs:documentation>nicht täglich oder nicht dauerhaft</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="6">
				<xs:annotation>
					<xs:documentation>täglich, aber zusätzlich zur oralen
						Ernährung</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation>ausschließlich oder nahezu ausschließlich künstliche
						Ernährung</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="enum_harnkontinenz_type">
		<xs:annotation>
			<xs:documentation>Blasenkontrolle/Harnkontinenz</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:int">
			<xs:enumeration value="0">
				<xs:annotation>
					<xs:documentation>ständig kontinent</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>überwiegend kontinent. Maximal 1x täglich inkontinent oder
						Tröpfchen-/Stressinkontinenz</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation>überwiegend (mehrmals täglich) inkontinent, gesteuerte
						Blasenentleerung ist aber noch möglich</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation>komplett inkontinent, gesteuerte Blasenentleerung ist nicht
						möglich</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="4">
				<xs:annotation>
					<xs:documentation>Bewohner bzw. Bewohnerin hat einen Dauerkatheter oder ein
						Urostoma</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="enum_stuhlkontinenz_type">
		<xs:annotation>
			<xs:documentation>Darmkontrolle/Stuhlkontinenz</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:int">
			<xs:enumeration value="0">
				<xs:annotation>
					<xs:documentation>ständig kontinent</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>überwiegend kontinent, gelegentlich
						inkontinent</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation>überwiegend inkontinent, selten gesteuerte
						Darmentleerung</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation>komplett inkontinent, gesteuerte Darmentleerung ist nicht
						möglich</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="4">
				<xs:annotation>
					<xs:documentation>Bewohner bzw. Bewohnerin hat ein Colo- oder
						Ileostoma</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="enum_dekubitusStadium_type">
