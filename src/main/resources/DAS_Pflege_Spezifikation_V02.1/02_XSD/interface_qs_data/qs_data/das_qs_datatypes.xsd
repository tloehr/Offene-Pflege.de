				<xs:annotation>
					<xs:documentation>Kategorie/Stadium 2</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation>Kategorie/Stadium 3</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="4">
				<xs:annotation>
					<xs:documentation>Kategorie/Stadium 4</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="9">
				<xs:annotation>
					<xs:documentation>unbekannt</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
